<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ItemView">
   <children>
      <ImageView fx:id="itemImage" fitHeight="130.0" fitWidth="130.0" layoutX="35.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="itemName" alignment="CENTER" layoutX="50.0" layoutY="155.0" prefHeight="30.0" prefWidth="100.0" text="Label">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
